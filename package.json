{
  "name": "@famgia/omnify-react",
  "version": "0.0.2",
  "description": "React runtime components, hooks, and utilities for Omnify schemas",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./components": {
      "import": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/components/index.js"
      },
      "require": {
        "types": "./dist/components/index.d.cts",
        "default": "./dist/components/index.cjs"
      }
    },
    "./hooks": {
      "import": {
        "types": "./dist/hooks/index.d.ts",
        "default": "./dist/hooks/index.js"
      },
      "require": {
        "types": "./dist/hooks/index.d.cts",
        "default": "./dist/hooks/index.cjs"
      }
    },
    "./lib": {
      "import": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/lib/index.js"
      },
      "require": {
        "types": "./dist/lib/index.d.cts",
        "default": "./dist/lib/index.cjs"
      }
    },
    "./schemas": {
      "import": {
        "types": "./dist/schemas/index.d.ts",
        "default": "./dist/schemas/index.js"
      },
      "require": {
        "types": "./dist/schemas/index.d.cts",
        "default": "./dist/schemas/index.cjs"
      }
    },
    "./schemas/*": {
      "import": {
        "types": "./dist/schemas/*.d.ts",
        "default": "./dist/schemas/*.js"
      },
      "require": {
        "types": "./dist/schemas/*.d.cts",
        "default": "./dist/schemas/*.cjs"
      }
    }
  },
  "files": [
    "dist",
    "scripts",
    "README.md"
  ],
  "scripts": {
    "build:schemas": "tsx scripts/build-schemas.ts",
    "build:lib": "tsup",
    "build": "pnpm build:schemas && pnpm build:lib",
    "clean": "rm -rf dist src/schemas",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "postinstall": "node scripts/postinstall.cjs"
  },
  "keywords": [
    "omnify",
    "react",
    "typescript",
    "antd",
    "zod",
    "form",
    "validation",
    "i18n"
  ],
  "author": "Famgia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ecsol/omnify-ts.git",
    "directory": "packages/omnify-react"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.0"
  },
  "peerDependencies": {
    "antd": "^6.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "antd": {
      "optional": false
    },
    "zod": {
      "optional": false
    }
  },
  "devDependencies": {
    "@ant-design/icons": "^6.0.0",
    "@famgia/omnify-core": "workspace:*",
    "@famgia/omnify-typescript": "workspace:*",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "antd": "^6.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "zod": "^4.3.5"
  }
}
